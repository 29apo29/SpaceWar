<Window x:Class="SpaceWar3.GameGUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaceWar3"
        mc:Ignorable="d"
        Closed="On_Closed"
        Icon="pack://application:,,,/Resources/img/SpaceShip.ico"
        Title="GAME" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Background="Black">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="421*"/>
            <ColumnDefinition Width="379*"/>
        </Grid.ColumnDefinitions>
        <Canvas Name="starGrid"/>
        <Canvas Name="guiGrid" VerticalAlignment="Top" HorizontalAlignment="Left"/>

    </Grid>
</Window>
